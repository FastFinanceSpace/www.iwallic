		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^1.1.11",
		"html-webpack-plugin": "^3.2.0",
		"http-server": "^0.11.1",
		"node-sass": "^4.9.0",
		"optimize-css-assets-webpack-plugin": "3.2.0",
		"rimraf": "^2.6.2",
		"sass-loader": "^7.0.1",
		"style-loader": "^0.21.0",
		"to-string-loader": "^1.1.5",
		"uglifyjs-webpack-plugin": "^1.2.5",
		"webpack": "3.11.0",
		"webpack-dev-server": "2.7.1"
	}
