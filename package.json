	"license": "GPL-3.0",
	"private": true,
	"scripts": {
		"http": "http-server dist -p 3001 -c-1 --cors",
		"build": "rimraf ./dist && yarn webpack --config build/webpack.config.js --progress --profile --bail",
		"webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
		"prod": "yarn build && http-server dist -p 3001 -c-1 --cors",
		"dev": "yarn webpack-dev-server --config build/webpack.dev.config.js --open --progress --profile --watch --content-base src/ --host 0.0.0.0",
		"webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js"
	},
	"dependencies": {
		"jquery": "^3.3.1"
	},
	"devDependencies": {
		"copy-webpack-plugin": "^4.5.1",
